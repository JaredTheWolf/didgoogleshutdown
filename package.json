{
  "name": "DidGoogleShutDown",
  "version": "0.0.1",
  "description": "Google Watch",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "./node_modules/.bin/webpack",
    "start": "npm run dev",
    "lint": "eslint src --ignore-pattern src/views/*.spec.js",
    "dev": "node --harmony bin/webpack-dev-server",
    "dev:nw": "npm run dev -- --nw",
    "dev:no-debug": "npm run dev -- --no_debug",
    "test": "npm run test:unit && npm run test:server",
    "test:unit": "node ./node_modules/karma/bin/karma start",
    "test:server": "mocha --harmony bin/test-server",
    "deploy": "npm run compile",
    "postinstall": "npm run deploy",
    "server:start": "node --harmony bin/server",
    "server:dev": "nodemon --harmony --watch server bin/server",
    "static": "node ./bin/static",
    "prod-test": "npm run compile && node ./bin/static"
  },
  "authors": [
    "PK <pleasurekevin@didgoogleshutdown.com> (https://twitter.com/pleasure__kevin)",
    "Rainycode <info@rainycode.com> (https://rainycode.com)"
  ],
  "engines": {
    "node": "0.12.7"
  },
  "dependencies": {
